<table ng-controller="footballAppCtrl">
  <thead>
    NFL Scores - Week 13
    <tr>
      <th></th>
      <th>Home</th>
      <th></th>
      <th></th>
      <th>Away</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <tr style="width: 25px" ng-repeat="game in GameList">
      <td>
        <img ng-src="{{game.home_gif}}" />
      </td>
      <td>{{game.home_team}}</td>
      <td>{{game.home_score}}</td>
      <td>{{game.away_score}}</td>
      <td>{{game.away_team}}</td>
      <td>
        <img ng-src="{{game.away_gif}}" />
      </td>
    </tr>
  </tbody>
</table>

