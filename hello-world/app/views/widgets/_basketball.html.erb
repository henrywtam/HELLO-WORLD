<table id="basketbally" ng-controller="basketballAppCtrl" style="margin: 0px 10px;">
  <thead >
    <div style="font-size: 20px; text-align: center;">NBA Scores - <%= Time.now.strftime("%B %d") %></div>
    <br>
    <tr>
      <th style="width: 25px"></th>
      <th style="width: 90px; text-align: center">Home</th>
      <th style="width: 30px"></th>
      <th style="width: 30px"></th>
      <th style="width: 90px; text-align: center">Away</th>
      <th style="width: 25px"></th>
    </tr>
  </thead>
  <tbody>
    <tr ng-repeat="game in GameList">
      <td>
        <img style="width: 30px" ng-src="{{game.home_gif}}" />
      </td>
      <td style="font-size: 14px; text-align: center">{{game.home_team}}</td>
      <td style="font-size: 14px">{{game.home_score}}</td>
      <td style="font-size: 14px">{{game.away_score}}</td>
      <td style="font-size: 14px; text-align: center">{{game.away_team}}</td>
      <td>
        <img style="width: 30px" ng-src="{{game.away_gif}}" />
      </td>
    </tr>
  </tbody>
</table>
