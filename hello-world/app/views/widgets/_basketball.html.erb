<table id="basketbally" ng-controller="basketballAppCtrl">
  <thead>
    NBA Scores - <%= Date.yesterday %>
    <tr>
      <th></th>
      <th>Home</th>
      <th></th>
      <th></th>
      <th>Away</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <tr ng-repeat="game in GameList">
      <td>
        <img style="width: 35px" ng-src="{{game.home_gif}}" />
      </td>
      <td>{{game.home_team}}</td>
      <td>{{game.home_score}}</td>
      <td>{{game.away_score}}</td>
      <td>{{game.away_team}}</td>
      <td>
        <img style="width: 35px" ng-src="{{game.away_gif}}" />
      </td>
    </tr>
  </tbody>
</table>
